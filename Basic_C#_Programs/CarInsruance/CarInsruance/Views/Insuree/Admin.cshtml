
@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Quote Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var quote in Model)
        {
            <tr>
                <td>@quote.FirstName</td>
                <td>@quote.LastName</td>
                <td>@quote.Email</td>
                <td>@quote.QuoteAmount.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>